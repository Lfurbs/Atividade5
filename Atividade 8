#include<stdio.h>

main(){
  
    int idade, servico;

    printf("Quantos anos voce tem?");
    scanf("%d", &idade);

    printf("A quantos anos voce esta prestando serviÃ§os");
    scanf("%d", &servico);


    if (idade >=65 || servico >= 30)
    {
        printf("Voce esta apto a aposentar");
    }
    else if ( idade >= 60 && servico >= 25)
    {
       printf("Voce esta apto a aposentar");
    }
    else{
        printf("Voce nao esta apto a aposentar");
    }
}
